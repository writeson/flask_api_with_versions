  "version": "0.2.0",
  "configurations": [
    {
      "name": "init_db",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },

    {
      "name": "Rectangle App",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "FLASK_APP": "${workspaceFolder}/project/rectangle.py",
        "FLASK_ENV": "development"
      },
      "args": ["run"],
      "justMyCode": false,
      "jinja": true
    }
  ]
}
